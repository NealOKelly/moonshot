<div id="upload-form-container" class="upload-form-hidden">
	<form id="upload-form">
		<label for="record-type-input">Document</label>
		<div class="custom-file mb-3">
			<input id="upload-form-file" name="upload" type="file" class="custom-file-input" id="validatedCustomFile">
			<label id="upload-form-file-label" class="custom-file-label" for="upload-form-file">Choose file...</label>
		</div>
		<div class="mb-3">
			<label for="upload-form-record-title">Record Title</label>
			<input id="upload-form-record-title" class="form-control" type="text">
		</div>
		<div class="mb-3">
			<label for="upload-form-record-container">Container</label>
			<input id="upload-form-record-container" class="form-control" type="text" data-record-uri="" readonly>
		</div>
		<div class="form-group">
		  <label for="upload-form-record-type">Record Type</label>
		  <select id="upload-form-record-type" class="form-control" readonly>
			<option>Schools Document</option>
		  </select>
		</div>
	</form>
	<button id="upload-button" class="btn btn-primary">Upload</button>
</div>			
	