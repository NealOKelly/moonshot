<div id="new-folder-form-container" class="new-folder-form-hidden" style="margin-left:5px;margin-right:5px;">
	<h3>New Folder</h3>
	<form id="new-folder-form">
		<div class="mb-3">
			<label for="new-folder-form-record-title">Folder Title</label>
			<input id="new-folder-form-record-title" class="form-control" type="text" maxlength="254">
		</div>
		<div class="mb-3">
			<label for="upload-form-record-classification">Classification</label>
			<input id="upload-form-record-classification" class="form-control" type="text" data-record-uri="" readonly>
		</div>
		<div class="form-group">
		  <label for="upload-form-record-type">Record Type</label>
		  <select id="new-folder-form-record-type" class="form-control">
			<option>Staff Folder</option>
			<option>Pupil Folder</option>
		  </select>
		</div>
	</form>
	<button id="upload-button" class="btn btn-primary">Create</button>
</div>			
	