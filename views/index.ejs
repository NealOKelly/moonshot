<div class="row" style="margin-top:56px;height:100%;">
	<div class="col-3" id="left-panel">
		<div id="classification-treeview" style="padding-top:30px;">
			<ul style="list-style-type:none;margin-left:-10px;display:inline-block;">
				<li id="favourites"><span class='collapsed'></span><span class='star'></span><span class='favourites'><a>Favourites</a></span></li>
				<li id="recents"><span class='collapsed'></span><span class='clock-history'></span><span class='recents'><a>Recents</a></span></li>
				<li id="all-files"><span class='collapsed'></span><span class='folder'></span><span class='all-files'><a>All Files</a></span><ul style="list-style-type:none;"></ul></li>
			</ul>
		</div>
	</div>
	<div class="col-6" style="background-color: #fff;height:100%; padding:0;overflow:hidden;">
		<div id="records-list-pane" style="height:75%;text-align:center;overflow:auto;overflow-x:hidden;">
			
		</div>
		<div id="search-results-pane" style="height:75%;text-align:center;overflow:auto;overflow-x:hidden;display:none;width: 100%;">
			<table id="search-results" class="table table-sm">
				<thead style="background-color:#ffffff;">
					<tr>
						<th style="text-align:left;padding-left:30px;">Type</th>
						<th id="th-record-number" data-type="string" class="sorted-down" style="text-align:left;">Record Number</th>
						<th id="th-record-title" style="text-align:left;" data-type="string" class="unsorted">Title</th><th id="th-date-registered" data-type="string" class="unsorted" style="text-align:left;">Record Type</th>
						<th>Download</th>
					</tr>
				</thead>
				<tbody>

				</tbody>
			</table>
		</div>
		<div id="properties-pane" style="height:26%;text-align:center;overflow:auto;overflow-x:hidden;background-color:#343a40;position:relative;display:none;">
		</div>
		<div id="properties-pane-placeholder" style="height:26%;text-align:center;overflow:auto;overflow-x:hidden;background-color:#343a40;position:relative;">
			
			<img id="properties-pane-logo" src="img/gilbyim-logo-inline-white.png" style="maxheight:100%;maxwidth:100%;width:auto;height:auto;position:absolute;top:0;bottom:0;left:0;right:0;margin:auto;">
			
		</div>
	</div>
	<div class="col-3" style="height:100%;padding:20px;background-color:#bbbbbb;">
		<div style="height:100%;">
			<!--<div style="height:100%;overflow:auto;overflow-x:hidden;">
				<a class="twitter-timeline" href="https://twitter.com/RT_GilbyIM?ref_src=twsrc%5Etfw">Tweets by GilbyIM</a>
				<script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
			</div>-->
			<%- include('./partials/upload-form.ejs') %>
			<%- include('./partials/new-folder-form.ejs') %>
			<%- include('./partials/new-sub-folder-form.ejs') %>
		</div>
	</div>
</div>